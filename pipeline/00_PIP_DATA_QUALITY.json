{
	"name": "00_PIP_DATA_QUALITY",
	"properties": {
		"activities": [
			{
				"name": "Etapa_Busca_Arquivo_DQ",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DS_ARQUIVO_DQ",
						"type": "DatasetReference",
						"parameters": {
							"arquivo_xlsx": "*"
						}
					},
					"fieldList": [
						"childItems"
					],
					"storeSettings": {
						"type": "FileServerReadSettings",
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "BinaryReadSettings"
					}
				}
			},
			{
				"name": "Etapa_Filtro_XLSX",
				"type": "Filter",
				"dependsOn": [
					{
						"activity": "Etapa_Busca_Arquivo_DQ",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Etapa_Busca_Arquivo_DQ').output.childItems",
						"type": "Expression"
					},
					"condition": {
						"value": "@endswith(item().name, '.xlsx')",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Etapa_Carga_Dados_Storage",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Etapa_Filtro_XLSX",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Etapa_Filtro_XLSX').output.Value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "DQ - Cadastro Fonte",
							"type": "DatabricksNotebook",
							"dependsOn": [
								{
									"activity": "Upload_File",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"notebookPath": "/Shared/WS_DATABRICKS/prj_data_quality/00_dq/02_dq_executa_cadastro_fonte_dado"
							},
							"linkedServiceName": {
								"referenceName": "LS_DATABRICKS",
								"type": "LinkedServiceReference"
							}
						},
						{
							"name": "Upload_File",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "BinarySource",
									"storeSettings": {
										"type": "FileServerReadSettings",
										"recursive": true
									},
									"formatSettings": {
										"type": "BinaryReadSettings"
									}
								},
								"sink": {
									"type": "BinarySink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "DS_ARQUIVO_DQ",
									"type": "DatasetReference",
									"parameters": {
										"arquivo_xlsx": {
											"value": "@item().name",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DS_STORAGE_ACCOUNT_XLSX_DQ",
									"type": "DatasetReference",
									"parameters": {
										"arquivo_xlsx": {
											"value": "item().name",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"folder": {
			"name": "data_quality"
		},
		"annotations": []
	}
}