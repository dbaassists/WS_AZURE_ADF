{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-treinamento-ws"
		},
		"LS_DATABRICKS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_DATABRICKS'"
		},
		"LS_DATA_LAKE_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DATA_LAKE'"
		},
		"LS_SQL_SERVER_ONPREMISE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_SERVER_ONPREMISE'"
		},
		"LS_DATABRICKS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0416-221816-6puiu1cx"
		},
		"LS_DATA_LAKE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://asatreinamentows.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-treinamento-ws_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_DATABRICKS_accessToken": {
						"value": "[parameters('LS_DATABRICKS_accessToken')]"
					},
					"LS_DATA_LAKE_accountKey": {
						"value": "[parameters('LS_DATA_LAKE_accountKey')]"
					},
					"LS_SQL_SERVER_ONPREMISE_connectionString": {
						"value": "[parameters('LS_SQL_SERVER_ONPREMISE_connectionString')]"
					},
					"LS_DATABRICKS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_DATABRICKS_properties_typeProperties_existingClusterId')]"
					},
					"LS_DATA_LAKE_properties_typeProperties_url": {
						"value": "[parameters('LS_DATA_LAKE_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}