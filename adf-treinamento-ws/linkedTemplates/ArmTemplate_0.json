			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/00_PIP_EXTRATOR_DADOS_CLIENTE')]",
				"[concat(variables('factoryId'), '/linkedServices/LS_DATABRICKS')]"
			]
		}
	]
}